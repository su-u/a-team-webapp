{
  "name": "a-team-webapp",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/su-u/a-team-webapp.git",
  "author": "su- <aaaiiiuuu35@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "imutable": "^0.0.1",
    "linq": "^3.2.1",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "react-script": "^2.0.5",
    "redux": "^4.0.4",
    "styled-components": "^4.4.1",
    "webpack": "^4.41.2"
  },
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-react": "^7.7.0",
    "@types/mocha": "^5.2.7",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.4",
    "@types/react-redux": "^7.1.5",
    "@types/react-router-dom": "^5.1.2",
    "@types/styled-components": "^4.4.0",
    "babel-loader": "^8.0.6",
    "prettier": "^1.19.1",
    "ts-loader": "^6.2.1",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^8.0.1",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.7.2",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.7.2",
    "webpack-merge": "^4.2.2",
    "yarn-run-all": "^3.1.1"
  },
  "scripts": {
    "webpack:development": "webpack --config ./webpack.dev.js --display-error-details --progress",
    "webpack:production": "webpack --config ./webpack.pro.js --display-error-details --progress",
    "development": "yarn run-p webpack:development",
    "production": "yarn run-p webpack:production",
    "docker:production": "docker-compose up --build -d & yarn webpack:production",
    "docker:stop": "docker stop $(docker ps -a -q)",
    "watch": "webpack --config ./webpack.dev.js --display-error-details --progress --watch --color --info-verbosity verbose",
    "check": "yarn run tsc --noEmit && yarn run tslint",
    "tslint-fix": "tslint --fix --project tsconfig.json",
    "tslint:prettier-check": "tslint-config-prettier-check ./tslint.json",
    "tslint": "tslint --fix --force --format stylish --project ./tsconfig.json ./{src,__tests__}/**/*.{ts,tsx}",
    "prettier": "prettier --write ./{src,__tests__}/**/*.{ts,tsx}",
    "start": "react-scripts-ts start",
    "build": "react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject"
  }
}
